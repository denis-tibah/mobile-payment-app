{
  "expo": {
    "name": "zazoostg",
    "scheme": "zazoomobilestg",
    "slug": "zazoo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/zazoosplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSFaceIDUsageDescription": "We need your permission to authenticate using Face ID"
      },
      "bundleIdentifier": "com.zazoostg.zazoomobilestg"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.zazoostg.zazoomobilestg",
      "permissions": [
        "INTERNET",
        "android.permission.SECURE_STORAGE"
      ],
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
             {
               "scheme": "https",
               "host": "www.gozazoo.com/",
               "pathPrefix": "/"
             },
             {
               "scheme": "zazoomobilestg",
               "host": "*",
               "pathPrefix": "/"
             }
           ],
         "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
       }
    ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7454c009-85fa-4eb0-ae8c-5834ded616c2"
      }
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow zazoo to use Face ID."
        }
      ],
      [
        "expo-image-picker",
        {
          "photoPermission": "The app accesses your photos to let you share them"
        }
      ]
    ]
  }
}